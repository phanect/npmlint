{
  "name": "npmlint",
  "version": "1.0.0",
  "description": "Lint your package before publishing npm packages",
  "keywords": [
    "lint",
    "linter",
    "npm"
  ],
  "author": {
    "name": "Jumpei Ogawa",
    "url": "https://phanective.org"
  },
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/main.mjs",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    }
  },
  "devDependencies": {
    "@phanect/configs": "latest",
    "@phanect/lint": "latest",
    "@types/node": "^24.0.3",
    "eslint": "^9.11.0",
    "jiti": "latest",
    "tsdown": "0.16.8",
    "typescript": "^5.6.2",
    "vitest": "^4.0.4"
  },
  "engines": {
    "node": "^20.11.0 || ^22.16.0 || >=24.0.0"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
